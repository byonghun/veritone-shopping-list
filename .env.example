NODE_ENV=development
PORT=3001
# Prisma / Postgres
# IMPORTANT: host is the compose service name "db" (internal Docker DNS), not localhost.
# Example: DATABASE_URL=postgresql://veritoneuser:veritonepassword@db:5432/veritonedb?schema=public
DATABASE_URL=postgresql://<POSTGRES_USER>:<POSTGRES_PASSWORD>@db:<PORT>/<POSTGRES_DB>?schema=public